#include<bits/stdc++.h>
using namespace std;

int check(int n){
    if(n<0) return -n;
    else return n;
}

int catch(int a, int b, int c){
    if(check(c-a) < check(c-b)) return 0;
    else if(check(c-a) > check(c-b)) return 1;
    else return 2;
}

int main() {
    int q,i,j;
    cin>>q;
    int x[q][3],r;
    
    for(i=0;i<q;i++){
        for(j=0;j<3;j++){
            cin>>x[i][j];
        }
    }
    
    for(i=0;i<q;i++){
        
        r = catch(x[i][0],x[i][1],x[i][2]);
        if(r == 0) cout<<"Cat A\n";
        else if(r == 1) cout<<"Cat B\n";
        else cout<<"Mouse C";
    }
    return 0;
}
